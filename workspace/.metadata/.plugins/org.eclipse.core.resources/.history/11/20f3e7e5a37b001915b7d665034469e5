package com.bit;

import java.util.Scanner;

interface Inter{
	void on();
}
class Tv implements Inter{
	public void on() {
		System.out.println("tv를 켜다");
	}
}
class Radio{
	public void on() {
		System.out.println("Radio를 켜다");
	}
}
class Audio{
	public void on() {
		System.out.println("Audio를 켜다");
	}
}

public class Remote {
	public static void main(String[] args){
		Scanner sc = new Scanner(System.in);
	}

}
